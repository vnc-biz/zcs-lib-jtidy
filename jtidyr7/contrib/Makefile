all: JTidy

JTidy: $(DOMCLASSES) $(JTIDYCLASSES) $(LIBDIR)/Tidy.jar

$(LIBDIR)/Tidy.jar: $(DOMCLASSES) $(JTIDYCLASSES) \
$(DOCDIR)/images/tidy.gif \
$(SRCDIR)/$(TIDYPKG_DIR)/TidyMessages.properties
    cp $(DOCDIR)/images/tidy.gif $(LIBDIR)/org/w3c/tidy
    cp $(SRCDIR)/org/w3c/tidy/TidyMessages.properties $(LIBDIR)/org/w3c/tidy
    cd $(LIBDIR);jar cfm Tidy.jar manifest.txt $(TIDYPKG_PATH)*.* $(DOMPKG_PATH)*.*

clean:
    rm -f $(LIBDIR)/Tidy.jar
    rm -f $(LIBDIR)/org/w3c/tidy/*.class
    rm -f $(LIBDIR)/org/w3c/tidy/*.properties
    rm -f $(LIBDIR)/org/w3c/dom/*.class
    rm -f $(DOCDIR)/*.html


